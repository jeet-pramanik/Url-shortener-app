  <script>
    let countdown = 20;
    let interval;

    function startTimer() {
      document.querySelector('.start-button').disabled = true;
      interval = setInterval(() => {
        countdown -= 1;
        document.getElementById('timer').textContent = countdown;
        if (countdown <= 0) {
